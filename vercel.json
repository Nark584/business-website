{
  "version": 2,
  "name": "japanese-life-guide",
  "buildCommand": "echo 'Static site - no build required'",
  "outputDirectory": ".",
  "functions": {
    "api/visitors.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
